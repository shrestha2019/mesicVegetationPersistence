// print("grid50", grid50.sort('wetProp'))

var grid40 = gridWet.filter(ee.Filter.gt('wetProp', 0.005650102440277418))
  .filter(ee.Filter.lt('wetProp', 0.04518928641640883))
print("grid40", grid40.sort('wetProp'))

// var grid20 = gridWet.filter(ee.Filter.gt('wetProp', 0.0002358137925516106))
