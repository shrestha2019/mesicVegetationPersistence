// // Randomly select a NAIP image that intersects each of the sampled blocks 
var Wettest0 = s2.filterBounds(ee.Feature(listWettest.get(0)).geometry().centroid({'maxError': 1}))
  .filter(ee.Filter.calendarRange(5, 10, 'month'))
  .filter(ee.Filter.calendarRange(2017, 2020, 'year'))
  .filter(ee.Filter.lt('CLOUDY_PIXEL_PERCENTAGE',2))
  .randomColumn().sort('random').first()
print("Wettest0", Wettest0)

