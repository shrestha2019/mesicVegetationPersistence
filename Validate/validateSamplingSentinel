// // // Samples - 10 blocks from each stratum - changed the seeds until ZERO blocks were over known lava flow areas
var wettest = ee.FeatureCollection(grid80.randomColumn('random', 17).sort('random').toList(10))
// var wet = ee.FeatureCollection(grid50.randomColumn('random', 48).sort('random').toList(10))
// var dry = ee.FeatureCollection(grid40.randomColumn('random', 92).sort('random').toList(10))
