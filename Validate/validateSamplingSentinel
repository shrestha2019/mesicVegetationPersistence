/**** Start of imports. If edited, may not auto-convert in the playground. ****/
var s2 = ee.ImageCollection("COPERNICUS/S2_HARMONIZED");
/***** End of imports. If edited, may not auto-convert in the playground. *****/
var sageBiome = ee.FeatureCollection('users/ns_boise/US_Sagebrush_Biome_2019')
var mesicAZ = ee.FeatureCollection('users/ns_boise/mesicAZ')
var mesicCA = ee.FeatureCollection('users/ns_boise/mesicCA')
var mesicCAN = ee.FeatureCollection('users/ns_boise/mesicCAN')
var mesicCO = ee.FeatureCollection('users/ns_boise/mesicCO')
var mesicID = ee.FeatureCollection('users/ns_boise/mesicID')
var mesicMT = ee.FeatureCollection('users/ns_boise/mesicMT')
var mesicND = ee.FeatureCollection('users/ns_boise/mesicND')
var mesicNM = ee.FeatureCollection('users/ns_boise/mesicNM')
var mesicNV = ee.FeatureCollection('users/ns_boise/mesicNV')
var mesicOR = ee.FeatureCollection('users/ns_boise/mesicOR')
var mesicSD = ee.FeatureCollection('users/ns_boise/mesicSD')
var mesicUT = ee.FeatureCollection('users/ns_boise/mesicUT')
var mesicWA = ee.FeatureCollection('users/ns_boise/mesicWA')
var mesicWY = ee.FeatureCollection('users/ns_boise/mesicWY')

var merged = mesicAZ.merge(mesicCA).merge(mesicCAN).merge(mesicCO).merge(mesicID).merge(mesicMT).merge(mesicND).merge(mesicNM)
            .merge(mesicNV).merge(mesicOR).merge(mesicSD).merge(mesicUT).merge(mesicWA).merge(mesicWY)

// Map.addLayer(merged, {}, "merged")

// //--------------------------------------------------------------------------------------------//


var emptyRast = ee.Image().byte(); // <-- create empty raster for painting
var paintStudyarea = emptyRast.paint(sageBiome, '000000', 1);
// // draw a grid over area away from edges to avoid false "DRY" gridcells
var grid = sageBiome.geometry().coveringGrid('EPSG:4326', 20000)
Map.centerObject(grid, 6)

Map.addLayer(paintStudyarea, {color: 'purple'}, "Sage Brush Biome")
// Map.addLayer(grid, {}, "GRID")
print(grid.first())
// // Map.addLayer(lava, {color: 'red'}, "lava flows")
// // Map.addLayer(nwi, {}, "NWI")

// Try generating a binary raster from the NWI fc and aggregating the area per gridcell - faster
var wet = 1
var other = 0

var hdImg = ee.Image(other).clip(grid.geometry())

var wetImg = ee.Image(wet).clipToCollection(merged)
// print(wetImg)
var binaryWet = hdImg.where({test: wetImg, value: wetImg})
// Map.addLayer(binaryWet, {min:0, max:1}, 'Binary')

var gridWet = binaryWet.reduceRegions({
  collection: grid,
  reducer: ee.Reducer.sum(),
  scale: 30 // could lower, but maybe not necessary
})

var gridWet = gridWet.map(function(f){
  return f.set('wetProp',ee.Number(f.get('sum')).multiply(900).divide(400000000))
  
})
Map.addLayer(gridWet, {}, 'gridWet')


var hist = ui.Chart.feature.histogram({
  features: gridWet,
  property: 'wetProp',
  maxBuckets: 100
})

print(hist)

// print('summary stats',
//       gridWet.aggregate_stats('wetProp'));
// print('summary stats',
//       gridWet.aggregate_histogram('wetProp'));

var temp = gridWet.aggregate_array('wetProp')
// print(temp)
// var list = temp.toDictionary().values();
var percentiles = temp.reduce(ee.Reducer.percentile([25, 43, 50, 60, 70, 80]));

// .map(function(feat){
//   var list = feat.toDictionary().values();
//   var percentiles = list.reduce(ee.Reducer.percentile([20,30,45]));
//   return ee.Feature(feat.geometry(), percentiles);
// });
print(percentiles)

// // var bins = [ 5919, 9327, 16330]

var grid80 = gridWet.filter(ee.Filter.gt('wetProp', 0.15009173441176235))//75 percentile
print("grid80", grid80.sort('wetProp'))

// var grid80 = gridWet.filter(ee.Filter.gt('wetProp', 0.15009173441176235))
//   .filter(ee.Filter.lt('wetProp', 0.24430212573528634))
// print("grid80", grid80.sort('wetProp'))

// var grid70 = gridWet.filter(ee.Filter.gt('wetProp', 0.08404029065743948))
//   .filter(ee.Filter.lt('wetProp', 0.15009173441176235))
// print("grid70", grid70.sort('wetProp'))

var grid60 = gridWet.filter(ee.Filter.gt('wetProp', 0.04518928641640883))
  .filter(ee.Filter.lt('wetProp', 0.15009173441176235))
print("grid60", grid60.sort('wetProp'))

// var grid50 = gridWet.filter(ee.Filter.gt('wetProp', 0.017464225649794884))
