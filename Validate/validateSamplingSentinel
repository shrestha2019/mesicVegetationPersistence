// print("grid20", grid20.sort('wetProp'))

var grid00 = gridWet.filter(ee.Filter.lt('wetProp', 0.0002358137925516106))
print("grid00", grid00.sort('wetProp'))
// var grid75 = gridWet.filter(ee.Filter.gt('wetProp', 0.11521687830882257))//75 percentile
// print("Wettest", grid75.sort('wetProp'))

